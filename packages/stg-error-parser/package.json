{
  "name": "@stargatefinance/stg-error-parser",
  "version": "0.1.3",
  "license": "BUSL-1.1",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath clean-prebuild && $npm_execpath populate && $npm_execpath tsup",
    "clean": "rm -rf dist .turbo",
    "clean-prebuild": "rm -rf dist 'src/errors/*.json'",
    "populate": "ts-node scripts/populate.ts && $npm_execpath prettier --write 'src/errors/*.json'",
    "test": "jest"
  },
  "dependencies": {
    "@layerzerolabs/evm-sdks-core": "~2.3.20"
  },
  "devDependencies": {
    "@layerzerolabs/tsup-config-next": "~2.3.10",
    "@layerzerolabs/typescript-config-next": "~2.3.10",
    "@stargatefinance/stg-evm-sdk-v2": "*",
    "@types/jest": "^29.5.10",
    "@types/node": "^18.15.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "~5.4.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
