{
  "name": "@stargatefinance/stg-evm-sdk-v2",
  "version": "1.0.0",
  "license": "BUSL-1.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./deployed": {
      "types": "./dist/deployed/index.d.ts",
      "import": "./dist/deployed/index.mjs",
      "require": "./dist/deployed/index.cjs"
    },
    "./deployed/*": {
      "types": "./dist/deployed/*.d.ts",
      "import": "./dist/deployed/*.mjs",
      "require": "./dist/deployed/*.cjs"
    },
    "./artifacts/*": {
      "import": "./artifacts/*",
      "require": "./artifacts/*"
    },
    "./deployments/*": {
      "import": "./deployments/*",
      "require": "./deployments/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "artifacts/**/*",
    "deployments/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath tsup --clean",
    "clean": "rm -rf .turbo dist artifacts deployments"
  },
  "devDependencies": {
    "@layerzerolabs/tsup-config-next": "~2.3.10",
    "@layerzerolabs/typescript-config-next": "~2.3.10",
    "@stargatefinance/stg-evm-v2": "~1.0.0",
    "@types/node": "^18.15.11",
    "esbuild-plugin-clean": "~1.0.1",
    "esbuild-plugin-copy": "~2.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "~5.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
