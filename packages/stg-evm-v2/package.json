{
  "name": "@stargatefinance/stg-evm-v2",
  "version": "1.0.12",
  "license": "BUSL-1.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./deployed": {
      "types": "./dist/deployed/index.d.ts",
      "import": "./dist/deployed/index.mjs",
      "require": "./dist/deployed/index.cjs"
    },
    "./deployed/*": {
      "types": "./dist/deployed/*.d.ts",
      "import": "./dist/deployed/*.mjs",
      "require": "./dist/deployed/*.cjs"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "artifacts",
    "artifacts-zk",
    "deployments",
    "deployments-zk",
    "src"
  ],
  "scripts": {
    "build": "$npm_execpath build:exports && $npm_execpath build:ts",
    "build:exports": "npx @layerzerolabs/export-deployments -o deployed",
    "build:ts": "$npm_execpath tsc --noEmit && $npm_execpath tsup --clean",
    "clean": "rm -rf cache out artifacts dist deployed ts-src/typechain-types",
    "compile": "concurrently 'pnpm:compile:*'",
    "compile:forge": "forge build",
    "compile:hardhat": "$npm_execpath hardhat compile",
    "compile:hardhat:evm": "$npm_execpath hardhat compile",
    "compile:hardhat:zksync": "$npm_execpath hardhat compile --network zksync-mainnet",
    "coverage:forge": "forge coverage --no-match-path ./test/invariant/StargateInvariantTest.t.sol --report lcov",
    "forge": "forge",
    "format-sol": "prettier --no-error-on-unmatched-pattern --ignore-unknown -w ./src/**/*.sol",
    "genhtml:coverage:forge": "genhtml lcov.info --branch-coverage --output-dir coverage --ignore-errors category",
    "hardhat": "hardhat",
    "lint": "$npm_execpath format-sol && solhint ./src/**/*.sol",
    "snapshot:build-and-push": "STG_EVM_NODE_TARGET=stargate-node docker buildx bake -f ./docker-compose.yaml --set '*.platform=linux/amd64,linux/arm64' --push",
    "snapshot:retrieve": "$npm_execpath snapshot:retrieve:bsc && $npm_execpath snapshot:retrieve:ethereum && $npm_execpath snapshot:retrieve:polygon",
    "snapshot:retrieve::network": "rm -rf snapshot/$NETWORK_NAME && mkdir -p snapshot/$NETWORK_NAME && docker compose cp $NETWORK_NAME:/app/cache ./snapshot/$NETWORK_NAME/cache",
    "snapshot:retrieve:bsc": "NETWORK_NAME=bsc $npm_execpath snapshot:retrieve::network",
    "snapshot:retrieve:ethereum": "NETWORK_NAME=ethereum $npm_execpath snapshot:retrieve::network",
    "snapshot:retrieve:polygon": "NETWORK_NAME=polygon $npm_execpath snapshot:retrieve::network",
    "start": "$npm_execpath start:localnet",
    "start:localnet": "STG_EVM_NODE_TARGET=layerzero-node docker compose up",
    "start:snapshot": "STG_EVM_NODE_TARGET=stargate-node docker compose up",
    "stop": "docker compose down",
    "test": "concurrently --group 'pnpm:test:*'",
    "test:forge": "forge test",
    "test:hardhat": "hardhat test"
  },
  "dependencies": {
    "@layerzerolabs/lz-definitions": "~2.3.25",
    "@layerzerolabs/lz-evm-messagelib-v2": "2.0.11",
    "@layerzerolabs/lz-evm-oapp-v2": "~2.3.25",
    "@layerzerolabs/lz-evm-protocol-v2": "2.0.11",
    "@layerzerolabs/lz-evm-sdk-v2": "~2.3.25",
    "@layerzerolabs/lz-evm-v1-0.7": "~2.3.25",
    "@layerzerolabs/lz-utilities": "~2.3.25",
    "@layerzerolabs/lz-v2-utilities": "~2.3.25",
    "@stargatefinance/stg-definitions-v2": "~1.1.1",
    "@stargatefinance/stg-evm-v1": "~1.0.31",
    "@types/sinon": "~17.0.3",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@ensdomains/buffer": "^0.1.1",
    "@ethersproject/abi": "~5.7.0",
    "@ethersproject/abstract-provider": "~5.7.0",
    "@ethersproject/abstract-signer": "~5.7.0",
    "@ethersproject/address": "~5.7.0",
    "@ethersproject/bignumber": "~5.7.0",
    "@ethersproject/bytes": "~5.7.0",
    "@ethersproject/constants": "~5.7.0",
    "@ethersproject/contracts": "~5.7.0",
    "@ethersproject/hash": "~5.7.0",
    "@ethersproject/providers": "~5.7.2",
    "@layerzerolabs/devtools": "~0.3.18",
    "@layerzerolabs/devtools-evm": "~0.3.13",
    "@layerzerolabs/devtools-evm-hardhat": "~0.3.22",
    "@layerzerolabs/export-deployments": "~0.0.10",
    "@layerzerolabs/io-devtools": "~0.1.11",
    "@layerzerolabs/lz-evm-sdk-v1": "~2.3.25",
    "@layerzerolabs/protocol-devtools": "~0.3.9",
    "@layerzerolabs/protocol-devtools-evm": "~0.3.10",
    "@layerzerolabs/solidity-examples": "^1.1.0",
    "@layerzerolabs/test-devtools": "~0.2.6",
    "@layerzerolabs/toolbox-foundry": "~0.1.7",
    "@layerzerolabs/toolbox-hardhat": "~0.2.37",
    "@layerzerolabs/tsup-config-next": "~2.3.25",
    "@layerzerolabs/typescript-config-next": "~2.3.25",
    "@layerzerolabs/ua-devtools": "~0.3.20",
    "@layerzerolabs/ua-devtools-evm": "~0.3.15",
    "@layerzerolabs/ua-devtools-evm-hardhat": "~0.3.19",
    "@matterlabs/hardhat-zksync-deploy": "~0.9.0",
    "@matterlabs/hardhat-zksync-solc": "~1.1.4",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.8.1",
    "@openzeppelin/contracts-upgradeable": "^4.8.1",
    "@stargatefinance/stg-devtools-evm-hardhat-v2": "~0.0.1",
    "@stargatefinance/stg-devtools-v2": "~0.0.1",
    "@typechain/ethers-v5": "~11.1.2",
    "@typechain/hardhat": "~9.1.0",
    "@types/mocha": "~10.0.7",
    "@types/node": "^18.15.11",
    "@uniswap/permit2": "git+https://github.com/Uniswap/permit2.git",
    "@uniswap/permit2-sdk": "^1.2.0",
    "chai": "^4.3.7",
    "concurrently": "~8.2.2",
    "dotenv": "^16.4.5",
    "fp-ts": "~2.16.5",
    "hardhat": "^2.16.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.2",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-spdx-license-identifier": "^2.1.0",
    "p-memoize": "~4.0.4",
    "prettier": "^3.1.1",
    "sinon": "~17.0.1",
    "solc": "~0.8.22",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.12",
    "solmate": "git+https://github.com/transmissions11/solmate.git",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typechain": "~8.3.2",
    "typescript": "~5.5.3",
    "zod": "~3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
